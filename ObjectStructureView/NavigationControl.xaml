<UserControl x:Class="ObjectStructureView.NavigationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Orientation="Horizontal">
        <StackPanel Orientation="Vertical">
            <TextBlock />
            <Button Content="PageUp" Command="{Binding PageUpCommand}" />
            <Button Content="PageDown" Command="{Binding PageDownCommand}" />
        </StackPanel>
    </StackPanel>
</UserControl>